<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/scrollView" >
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.caigehazelwood.hchsguidetogood.LateStartBell">

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Go Back"
        android:id="@+id/button15"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:onClick="button15"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Late Start Wednesday"
        android:id="@+id/textView12"
        android:textSize="30dp"
        android:textColor="@color/abc_background_cache_hint_selector_material_light"
        android:background="#FF6F2323"
        android:layout_below="@+id/button15"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/textView19" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="1st Period: 8:30-9:18"
        android:id="@+id/textView13"
        android:textSize="30dp"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:background="@color/abc_background_cache_hint_selector_material_light"
        android:layout_below="@+id/textView12"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/textView16" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="2nd Period: 9:24-10:12"
        android:id="@+id/textView14"
        android:textSize="30dp"
        android:background="#FF6F2323"
        android:textColor="@color/abc_background_cache_hint_selector_material_light"
        android:layout_below="@+id/textView13"
        android:layout_alignStart="@+id/textView13"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="3rd Period: 10:18-11:06"
        android:id="@+id/textView15"
        android:textSize="30dp"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:background="@color/abc_background_cache_hint_selector_material_light"
        android:layout_below="@+id/textView14"
        android:layout_alignStart="@+id/textView13"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="4th Period: 11:12-12:42"
        android:id="@+id/textView16"
        android:textSize="30dp"
        android:background="#FF6F2323"
        android:textColor="@color/abc_background_cache_hint_selector_material_light"
        android:layout_below="@+id/textView15"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="5th Period: 12:48-1:37"
        android:id="@+id/textView17"
        android:textSize="30dp"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:background="@color/abc_background_cache_hint_selector_material_light"
        android:layout_below="@+id/textView16"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/textView19" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="6th Period: 1:43-2:31"
        android:id="@+id/textView18"
        android:textSize="30dp"
        android:background="#FF6F2323"
        android:textColor="@color/abc_background_cache_hint_selector_material_light"
        android:layout_below="@+id/textView17"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/textView15" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="7th Period: 2:37-3:25"
        android:id="@+id/textView19"
        android:textSize="30dp"
        android:textColor="@color/abc_input_method_navigation_guard"
        android:background="@color/abc_background_cache_hint_selector_material_light"
        android:layout_below="@+id/textView18" />

</RelativeLayout>
</ScrollView>